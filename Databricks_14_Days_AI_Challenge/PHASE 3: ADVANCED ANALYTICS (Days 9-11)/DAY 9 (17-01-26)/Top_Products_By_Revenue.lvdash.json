{
  "datasets": [
    {
      "name": "736a7bf1",
      "displayName": "Top_Products_By_Revenue",
      "queryLines": [
        "SELECT\r\n",
        "    product_id,\r\n",
        "    category_code,\r\n",
        "    SUM(price) AS total_revenue\r\n",
        "FROM silver.events\r\n",
        "WHERE event_type = 'purchase'\r\n",
        "GROUP BY product_id, category_code\r\n",
        "ORDER BY total_revenue DESC\r\n",
        "LIMIT 10;\r\n"
      ]
    }
  ],
